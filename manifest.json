{
    "manifest_version": 3,
    "name": "Potluck",
    "description": "A simplified Youtube homepage based on peer recommendations",
    "version": "1.0",
    "host_permissions": [
        "https://www.youtube.com/*"
    ],
    "permissions" : [
        "identity",
        "identity.email",
        "webNavigation",
        "scripting",
        "tabs"
    ],
    "icons": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png",
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
    },
    "content_scripts": [
      {
        "js": [
          "scripts/homepage-script.js"
        ],
        "matches": [
            "https://www.youtube.com/"
        ]
      },
      {
        "js": [
          "scripts/recommend-script.js"
        ],
        "matches": [
            "https://www.youtube.com/watch?v=*"
        ]
      }
    ],
    "action": {
        "default_title": "FriendBlock, friends face's in a block."
    },
    "background": {
      "service_worker": "scripts/firebase.js"
    },
    "key": "",
    "oauth2": {
        "client_id": "644458548318-8ockpnvvcq0s8rdbiinuisa6i0huvg5l.apps.googleusercontent.com",
        "scopes" : [
            "https://www.googleapis.com/auth/userinfo.email",
            "https://www.googleapis.com/auth/contacts"
        ]
    }
}